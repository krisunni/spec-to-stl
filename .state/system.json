{
  "version": "2.0.0",
  "lastUpdated": "2026-02-09",
  "name": "Krypton NAS",
  "description": "3D printable Raspberry Pi 5 NAS enclosure with DC Comics Krypton/Fortress of Solitude theme, passive thermal management, and removable magnetic panels",
  "status": "operational",
  "environments": {
    "freecad": {
      "app": "/Applications/FreeCAD.app/Contents/MacOS/FreeCAD",
      "script": "krypton_nas_panels.py"
    },
    "openscad": {
      "file": "PiNasCase.scad"
    },
    "print": {
      "slicer": "PrusaSlicer / Cura",
      "material": "PLA / PETG",
      "nozzle": "0.4mm"
    }
  },
  "architecture": {
    "type": "parametric-cad",
    "language": "Python",
    "framework": "FreeCAD",
    "secondary": "OpenSCAD",
    "output": "STL, 3MF (multi-color)",
    "pipeline": "spec-driven",
    "viewer": "Three.js",
    "spec": ".state/spec.json"
  },
  "team": {
    "owner": "Kris",
    "repository": "case"
  }
}
